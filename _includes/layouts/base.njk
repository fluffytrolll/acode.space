    <header class="header">
      <a href="/" class="home-link">
        <span>Anpilogov</span>
        <span>Artem</span>
      </a>
      <nav class="nav">
        <input class="hide" type="checkbox" name="burger-input" id="burger-input" aria-label="Open menu">
        <label for="burger-input">
          <svg class="burger-open" width="28" height="28" aria-hidden="true">
            <use xlink:href="#burger" href="#burger"></use>
          </svg>
          <svg class="burger-close" width="28" height="28" aria-hidden="true">
            <use xlink:href="#close" href="#close"></use>
          </svg>
        </label>
        <ul class="nav-list">
          {%- for entry in collections.all | eleventyNavigation %}
            <li class="nav-list__item">
              <a href="{{ entry.url }}" {% if entry.url == page.url %} aria-current="page" {% endif %}>{{ entry.title }}</a>
            </li>
          {%- endfor %}
        </ul>
      </nav>
    </header>
    <main class="main">
      {{ content | safe }}
    </main>
    <footer class="footer"></footer>
  </body>
</html>